package com.cg;

public class Mobilescomperision {
	public static void main(String[] args) {
		int samsungA2 = 17000;
		int vivoY62 = 26000;
		System.out.println(Math.addExact(samsungA2, vivoY62));
		System.out.println(Math.subtractExact(vivoY62,samsungA2));
		System.out.println(samsungA2*3);
		
	}

}
