public class BiryanizoneExpenses {
	public static void main(String[] args) {
		int Familypackbiryani = 799;
		int regularbiryani = 339;
		int[] n =new n[]
		for(int i=0;i<n;i++) {
		System.out.println(n[i]);
		}
		System.out.println(Math.addExact(Familypackbiryani*2, regularbiryani*2)/7);
