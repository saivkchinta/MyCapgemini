package com.cg;
import java.util.*;
public class Expermentingonthings {

	public static void main(String[] args) {
	String firstName = args[0];
	String middleName = args[1];
	String lastName = args[2];
	System.out.println(firstName+" "+middleName+" "+lastName);
	int agein2021 = Integer.parseInt(args[3]);
	int agein2022 = Integer.parseInt(args[4]);
	int agein2023 = Integer.parseInt(args[5]);
	System.out.println(agein2021+" "+agein2022+" "+agein2023);
	/*
	 * System.out.println(middleName); System.out.println(lastName);
	 */
	}

}
